<!DOCTYPE html>
<html lang="en">
<head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
</head>
<body>

        
        <% if (messages.success) { %>
                <p class="alert alert-success mt-4"><%- messages.success %></p>
        <% } %>  
        <br>
        <table class="table">
                <thead>
                <tr>
                        <th scope="col">#</th>
                        <th scope="col">Unique ID</th>
                        <th scope="col">POLLING UNIT ID</th>
                        <th width="200px">POLLING UNIT NAME</th>
                        <th width="200px">POLLING UNIT DESCRIPTION</th>
                
                </tr>
                </thead>

        <tbody>
                <% if(data.length){
                
                for(var i = 0; i< data.length; i++) {%>  
                <tr>
                        <th scope="row"><%= (i+1) %></th>
                        <td><%= data[i].uniqueid%></td>
                        <td><%= data[i].polling_unit_id%></td>
                        <td><%= data[i].polling_unit_name%></td>
                        <td><%= data[i].polling_unit_description%></td>
                </tr>
                <% }
                        
                }else{ %>
                        <tr>
                        <td colspan="3">No user</td>
                        </tr>
                <% } %>    
        
        </tbody>
        
        </table>

        
</body>
</html>